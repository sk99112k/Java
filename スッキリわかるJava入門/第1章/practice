public class practice {
  /* 練習1-1 
   * ア,コンパイラ  イ,インタプリタ  ウ,コード  エ,バイトコード  オ,JVM */
  /* 練習1-2 */
  public static void main2(String[] args) {
    int a = 3;
    int b = 5;
    int c = a * b;
    System.out.println("縦幅3横幅5の長方形の面積は、" + c + "です");
  }

  /* 練習1-3 */
  boolean result = true;
  char oneCharacter = '駆';
  double pi = 3.14;
  long longNumber = 314159265853979L;
  String letter = "敵に15ポイントのダメージを与えた。";
}
